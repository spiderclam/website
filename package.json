{
	"name": "spiderclam",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"gen:cv": "pnpm run build && node tools/build-cv.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
